<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bkg"
    tools:ignore="ExtraText">

    <TextView
        android:id="@+id/CreateAccountText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="80dp"
        android:fontFamily="serif"
        android:text="Create Account"
        android:textColor="@color/white"
        android:textSize="23sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />


    <ImageView
        android:id="@+id/BackArrow_InCreate"
        android:layout_width="40dp"
        android:layout_height="36dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/ic_arrow_back"
        app:layout_constraintBottom_toBottomOf="@+id/CreateAccountText"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/CreateAccountText"
        tools:ignore="ContentDescription,MissingConstraints" />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/Round_AddImage"
        android:layout_width="78dp"
        android:layout_height="76dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="30dp"
        android:background="@drawable/round_imagge"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CreateAccountText"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/AddImageClick"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_add_a_photo"
            tools:ignore="ContentDescription,MissingConstraints" />
    </androidx.appcompat.widget.LinearLayoutCompat>

    <EditText
        android:id="@+id/MobileField"
        android:layout_width="360dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/textfield"
        android:ems="10"
        android:hint=" Mobile*"
        android:inputType="textPersonName"
        android:paddingStart="11dp"
        android:textColorHint="@color/black"
        android:drawableTint="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.514"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/UserNameField"
        android:autofillHints=""
        tools:ignore="HardcodedText,RtlHardcoded,RtlSymmetry" />

    <EditText
        android:id="@+id/Confirmpasswordfield"
        android:layout_width="360dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/textfield"
        android:drawableStart="@drawable/ic_lock"
        android:ems="10"
        android:hint=" Confirm Password"
        android:inputType="textPassword"
        android:paddingStart="11dp"
        android:textColorHint="@color/black"
        android:drawableTint="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/passwordfield"
        android:autofillHints=""
        tools:ignore="HardcodedText,RtlSymmetry" /><![CDATA[

    ]]><EditText
    android:id="@+id/passwordfield"
    android:layout_width="360dp"
    android:layout_height="50dp"
    android:layout_alignParentStart="true"
    android:layout_alignParentEnd="true"
    android:layout_alignParentBottom="true"
    android:layout_marginStart="8dp"
    android:layout_marginTop="32dp"
    android:background="@drawable/textfield"
    android:drawableStart="@drawable/ic_lock"
    android:ems="10"
    android:hint=" Password*"
    android:inputType="textPassword"
    android:paddingStart="11dp"
    android:textColorHint="@color/black"
    android:drawableTint="@color/black"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/emailField"
    android:autofillHints=""
    tools:ignore="HardcodedText,RtlSymmetry" />

    <EditText
        android:id="@+id/UserNameField"
        android:layout_width="360dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/textfield"
        android:drawableStart="@drawable/ic_user"
        android:ems="10"
        android:hint=" Full Name*"
        android:inputType="textPersonName"
        android:paddingStart="11dp"
        android:textColorHint="@color/black"
        android:drawableTint="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Round_AddImage"
        tools:ignore="Autofill,HardcodedText,RtlSymmetry" />

    <EditText
        android:id="@+id/emailField"
        android:layout_width="360dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/textfield"
        android:drawableLeft="@drawable/ic_email"
        android:ems="10"
        android:hint=" Email*"
        android:inputType="textPersonName"
        android:paddingLeft="11dp"
        android:textColorHint="@color/black"
        android:drawableTint="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.418"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MobileField"
        tools:ignore="Autofill,HardcodedText,RtlHardcoded,RtlSymmetry,TextFields" />

    <Button
        android:id="@+id/AccountCreate_button"
        android:layout_width="360dp"
        android:layout_height="54dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/textfield"
        android:text="Create"
        android:textColor="@color/black"
        android:drawableTint="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Confirmpasswordfield"
        tools:ignore="HardcodedText" />


</androidx.constraintlayout.widget.ConstraintLayout>